{
  "name": "flowcast",
  "private": true,
  "workspaces": [
    "./backend/",
    "./client/",
    "./infra/"
  ],
  "scripts": {
    "prepare": "yarn workspaces foreach -A run install",
    "deploy": "run prepare && yarn workspace @flowcast/infra run deploy"
  },
  "packageManager": "yarn@4.0.2"
}
